{
  "name": "inkpup-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "opennext:build": "npx --yes @opennextjs/cloudflare@latest build --config=open-next.config.cjs",
    "opennext:deploy": "npx --yes @opennextjs/cloudflare@latest deploy --config=wrangler.toml",
    "test:e2e": "playwright test",
    "test:integration": "npm run test:e2e",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:storybook": "vitest run --config vitest.workspace.ts --project=storybook",
    "test": "jest",
    "test:unit": "jest --coverage",
    "act:cache:warm": "bash scripts/act-cache-images.sh warm",
    "act:cache:load": "bash scripts/act-cache-images.sh load",
    "act:cache:list": "bash scripts/act-cache-images.sh list",
    "act:cache:prune": "bash scripts/act-cache-images.sh prune"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.653.0",
    "@tsparticles/react": "^3.0.0",
    "next": "^15.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sharp": "^0.33.5",
    "tsparticles": "^3.9.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@playwright/test": "^1.55.1",
    "@storybook/addon-vitest": "^9.1.9",
    "@storybook/nextjs-vite": "^9.1.9",
