# Local environment variables
# Copy this to .env and edit as needed

PORT=3002
PLAYWRIGHT_BASE_URL=http://localhost:3002
NODE_ENV=development
# Optional: base URL for Cloudflare R2 public bucket (no trailing slash)
# Example: https://your-account.r2.cloudflarestorage.com/bucket-name
NEXT_PUBLIC_R2_BASE_URL=
## Server-side R2 credentials (for signed URLs)
# These are secrets and must NOT be committed to source control. Set them in your deploy environment.
R2_ACCESS_KEY_ID=
R2_SECRET_ACCESS_KEY=
R2_ACCOUNT_ID=
R2_BUCKET=
